<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:DCGServiceDesk.View"
                    xmlns:vm="clr-namespace:DCGServiceDesk.ViewModels"
                    xmlns:vw="clr-namespace:DCGServiceDesk.View">

    <DataTemplate DataType="{x:Type vm:NotEscalatedViewModel}">
        <vw:NotEscalatedView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:EscalationViewModel}">
        <vw:EscalationView />
    </DataTemplate>

</ResourceDictionary>